&GLOBAL
  PROJECT Replica_20
  PRINT_LEVEL LOW
  RUN_TYPE ENERGY
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    CHARGE            0
    MULTIPLICITY      1
    BASIS_SET_FILE_NAME BASIS_MOLOPT_UZH
    POTENTIAL_FILE_NAME POTENTIAL_UZH
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-10
    &END QS
    &MGRID
       CUTOFF     1000
       REL_CUTOFF  60
    &END MGRID
    &SCF
      SCF_GUESS RESTART
      EPS_SCF 1.0E-6
      MAX_SCF 10
      &OT
        MINIMIZER DIIS
        PRECONDITIONER FULL_SINGLE_INVERSE
      &END
      &OUTER_SCF
         EPS_SCF 1.0E-6
         MAX_SCF 10
      &END
    &END SCF
    BASIS_SET_FILE_NAME BASIS_ADMM_UZH
    &XC
      &XC_FUNCTIONAL
         &MGGA_C_RSCAN
         &END MGGA_C_RSCAN
         &MGGA_X_RSCAN
         &END MGGA_X_RSCAN
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC  21.64 21.64 21.64
      ALPHA_BETA_GAMMA 90.0 90.0 90.0
      PERIODIC XYZ
    &END CELL
    &TOPOLOGY
      COORDINATE XYZ
      COORD_FILE_NAME rep_20.xyz
      CONNECTIVITY OFF
    &END
@include kinddef
  &END SUBSYS
&END FORCE_EVAL

