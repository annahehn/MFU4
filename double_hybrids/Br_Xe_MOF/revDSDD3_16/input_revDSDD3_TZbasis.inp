@SET MY_CUTOFF_RADIUS 10.0
&GLOBAL
  RUN_TYPE ENERGY
  PROJECT_NAME br_kr_revDSD
  PRINT_LEVEL MEDIUM
  FLUSH_SHOULD_FLUSH T
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL
&FORCE_EVAL
# STRESS_TENSOR ANALYTICAL
 METHOD QS
&DFT
   BASIS_SET_FILE_NAME BASIS_ADMM_UZH
   BASIS_SET_FILE_NAME BASIS_MOLOPT_UZH
   BASIS_SET_FILE_NAME BASIS_ccGRB_UZH
   POTENTIAL_FILE_NAME POTENTIAL_UZH
   CHARGE 0
   MULTIPLICITY 1
   AUTO_BASIS RI_AUX MEDIUM
   &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD BASIS_PROJECTION
      ADMM_PURIFICATION_METHOD NONE
      EXCH_SCALING_MODEL NONE
      EXCH_CORRECTION_FUNC PBEX
    &END
    &MGRID
      CUTOFF  900
      REL_CUTOFF  80
    &END MGRID
    &POISSON
      PERIODIC XYZ
      POISSON_SOLVER PERIODIC
    &END POISSON
  &QS
    METHOD GPW
    EPS_DEFAULT 1.0E-15
    EPS_PGF_ORB 1.0E-30
  &END QS
  &SCF
   SCF_GUESS RESTART
   MAX_SCF 50
   EPS_SCF 1.E-7
   &OT ON
     PRECONDITIONER FULL_ALL
     MINIMIZER DIIS
   &END
   &OUTER_SCF
     MAX_SCF 900
     EPS_SCF 1.E-7
   &END OUTER_SCF
  &END SCF
@include revDSDPBEP86D3.inc
 &END DFT  
  &SUBSYS
    &TOPOLOGY
      COORD_FILE_FORMAT  xyz
      COORD_FILE_NAME pbesol_rep16.xyz
      CONNECTIVITY OFF
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
     &END TOPOLOGY
    &CELL
      ABC  21.64 21.64 21.64
      ALPHA_BETA_GAMMA 90.0 90.0 90.0
      PERIODIC XYZ
    &END CELL
    &KIND H
      BASIS_SET  ccGRB-T-q1
      BASIS_SET  AUX_FIT admm-tzp-q1
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL  GTH-PBE0-q1
    &END KIND
    &KIND O
      BASIS_SET  ccGRB-T-q6
      BASIS_SET  AUX_FIT admm-tzp-q6
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL  GTH-PBE0-q6
    &END KIND
    &KIND C
      BASIS_SET ccGRB-T-q4
      BASIS_SET  AUX_FIT admm-tzp-q4
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q4
    &END KIND
    &KIND N
      BASIS_SET ccGRB-T-q5
      BASIS_SET  AUX_FIT admm-tzp-q5
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q5
    &END KIND
    &KIND Zn
      BASIS_SET ccGRB-T-q12
      BASIS_SET  AUX_FIT admm-tzp-q12
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q12
    &END KIND
    &KIND Cl
      BASIS_SET ccGRB-T-q7
      BASIS_SET  AUX_FIT admm-tzp-q7
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q7
    &END KIND
    &KIND Br
      BASIS_SET ccGRB-T-q7
      BASIS_SET  AUX_FIT admm-tzp-q7
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q7
    &END KIND
    &KIND Kr
      BASIS_SET ccGRB-T-q8
      BASIS_SET  AUX_FIT admm-tzp-q8
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q8
    &END KIND
    &KIND Xe
      BASIS_SET ccGRB-T-q8
      BASIS_SET  AUX_FIT admm-tzp-q8
  #    BASIS_SET  RI_AUX RI_TZ
      POTENTIAL GTH-PBE0-q8
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
