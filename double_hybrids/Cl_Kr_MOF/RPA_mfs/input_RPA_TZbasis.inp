&GLOBAL
  RUN_TYPE ENERGY
  PROJECT_NAME cl_kr_RPA
  PRINT_LEVEL MEDIUM
  FLUSH_SHOULD_FLUSH T
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL
&FORCE_EVAL
# STRESS_TENSOR ANALYTICAL
 METHOD QS
&DFT
   BASIS_SET_FILE_NAME BASIS_ADMM_UZH
   BASIS_SET_FILE_NAME BASIS_MOLOPT_UZH
   BASIS_SET_FILE_NAME BASIS_ccGRB_UZH
   POTENTIAL_FILE_NAME POTENTIAL_UZH
   CHARGE 0
   MULTIPLICITY 1
   AUTO_BASIS RI_AUX MEDIUM
   &AUXILIARY_DENSITY_MATRIX_METHOD
      METHOD BASIS_PROJECTION
      ADMM_PURIFICATION_METHOD NONE
      EXCH_SCALING_MODEL NONE
      EXCH_CORRECTION_FUNC PBEX
    &END
    &MGRID
      CUTOFF  900
      REL_CUTOFF  80
    &END MGRID
    &POISSON
      PERIODIC XYZ
      POISSON_SOLVER PERIODIC
    &END POISSON
  &QS
    METHOD GPW
    EPS_DEFAULT 1.0E-15
    EPS_PGF_ORB 1.0E-30
#   METHOD XTB
#   &XTB
#    CHECK_ATOMIC_CHARGES F
#    COULOMB_INTERACTION  T
#    DO_EWALD  T
#    HYDROGEN_STO_NG  4
#    STO_NG  6
#    TB3_INTERACTION T
#    USE_HALOGEN_CORRECTION T
#    DO_NONBONDED T
#    &NONBONDED
#     &GENPOT
#      ATOMS Kr Br
#      FUNCTION Aparam*exp(-Bparam*r)-Cparam/r**8
#      PARAMETERS Aparam Bparam Cparam
#      VALUES 132.192 2.066 0.001
#      VARIABLES r
#      RCUT 40.5
#     &END GENPOT
#    &END NONBONDED
#   &END XTB
#   &SE
#     &MEMORY
#       MAX_MEMORY 1000
#     &END MEMORY
#   &END SE
  &END QS
  &SCF
   SCF_GUESS RESTART
   MAX_SCF 50
   EPS_SCF 1.E-7
   &OT ON
     PRECONDITIONER FULL_ALL
     MINIMIZER DIIS
   &END
   &OUTER_SCF
     MAX_SCF 900
     EPS_SCF 1.E-7
   &END OUTER_SCF
  &END SCF
    &XC
      &XC_FUNCTIONAL
       &PBE
         SCALE_X 0.75
         SCALE_C 1.0
       &END PBE
      &END XC_FUNCTIONAL
      &vdW_POTENTIAL
          DISPERSION_FUNCTIONAL PAIR_POTENTIAL
          &PAIR_POTENTIAL
            TYPE DFTD3
       #     CALCULATE_C9_TERM .FALSE.
            PARAMETER_FILE_NAME dftd3.dat
            REFERENCE_FUNCTIONAL PBE0
            R_CUTOFF [angstrom] 10
          &END PAIR_POTENTIAL
      &END vdW_POTENTIAL
      &HF
        FRACTION 0.25
        &SCREENING
          EPS_SCHWARZ 1.0E-9
          SCREEN_ON_INITIAL_P FALSE
        &END SCREENING
        &INTERACTION_POTENTIAL
         POTENTIAL_TYPE TRUNCATED
         CUTOFF_RADIUS 10.0
        &END INTERACTION_POTENTIAL
      &MEMORY
       MAX_MEMORY 3000
      &END MEMORY       
      &END HF
      &WF_CORRELATION
        &LOW_SCALING
        &END
         &INTEGRALS
          ERI_METHOD GPW
          &WFC_GPW
            CUTOFF 150
            REL_CUTOFF 50
          &END WFC_GPW
        &END INTEGRALS
        &RI_RPA
          &HF
            FRACTION 1.00
            &SCREENING
              EPS_SCHWARZ 1.0E-8
              SCREEN_ON_INITIAL_P FALSE
            &END SCREENING
            &INTERACTION_POTENTIAL
             POTENTIAL_TYPE TRUNCATED
             CUTOFF_RADIUS 10.0
            &END INTERACTION_POTENTIAL
          &END HF
        &END RI_RPA
        MEMORY  3000
        NUMBER_PROC 4
      &END
    &END XC
 &END DFT  
  &SUBSYS
    &TOPOLOGY
      COORD_FILE_FORMAT  xyz
      COORD_FILE_NAME mfs_cl_kr_pbesol_optimized.xyz
      CONNECTIVITY OFF
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
     &END TOPOLOGY
    &CELL
      ABC  21.64 21.64 21.64
      ALPHA_BETA_GAMMA 90.0 90.0 90.0
      PERIODIC XYZ
    &END CELL
    &KIND H
      BASIS_SET  ccGRB-T-q1
      BASIS_SET  AUX_FIT admm-tzp-q1
      POTENTIAL  GTH-PBE0-q1
    &END KIND
    &KIND O
      BASIS_SET  ccGRB-T-q6
      BASIS_SET  AUX_FIT admm-tzp-q6
      POTENTIAL  GTH-PBE0-q6
    &END KIND
    &KIND C
      BASIS_SET ccGRB-T-q4
      BASIS_SET  AUX_FIT admm-tzp-q4
      POTENTIAL GTH-PBE0-q4
    &END KIND
    &KIND N
      BASIS_SET ccGRB-T-q5
      BASIS_SET  AUX_FIT admm-tzp-q5
      POTENTIAL GTH-PBE0-q5
    &END KIND
    &KIND Zn
      BASIS_SET ccGRB-T-q12
      BASIS_SET  AUX_FIT admm-tzp-q12
      POTENTIAL GTH-PBE0-q12
    &END KIND
    &KIND Cl
      BASIS_SET ccGRB-T-q7
      BASIS_SET  AUX_FIT admm-tzp-q7
      POTENTIAL GTH-PBE0-q7
    &END KIND
    &KIND Br
      BASIS_SET ccGRB-T-q7
      BASIS_SET  AUX_FIT admm-tzp-q7
      POTENTIAL GTH-PBE0-q7
    &END KIND
    &KIND Kr
      BASIS_SET ccGRB-T-q8
      BASIS_SET  AUX_FIT admm-tzp-q8
      POTENTIAL GTH-PBE0-q8
    &END KIND
    &KIND Xe
      BASIS_SET ccGRB-T-q8
      BASIS_SET  AUX_FIT admm-tzp-q8
      POTENTIAL GTH-PBE0-q8
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
